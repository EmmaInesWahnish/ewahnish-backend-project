<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: lightsteelblue;"> 
                <th>Id</th>
                <th>Nombre</th>
                <th>Precio</th> 
                <th>Imagen</th> 
            </tr>
            <%productos.forEach((product) => {%>
            <tr>
                <td><%=product.id%></td>
                <td><%=product.title%></td>
                <td><%=product.price%></td>
                <td><%=product.thumbnail%></td>
            </tr>
            <%});%>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">no se encontraron datos</h1>
<% } %>